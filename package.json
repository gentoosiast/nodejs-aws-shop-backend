{
  "name": "nodejs-aws-shop-backend",
  "version": "0.1.0",
  "bin": {
    "nodejs-aws-shop-backend": "bin/nodejs-aws-shop-backend.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "format": "prettier --write .",
    "cdk": "cdk",
    "deploy": "npm run build && cdk deploy",
    "destroy": "cdk destroy -f",
    "seed-db": "cross-env PYTHONUTF8=1 aws dynamodb batch-write-item --request-items file://./data/dynamodb-seed.json"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.138",
    "@types/jest": "^29.5.12",
    "@types/node": "20.14.2",
    "aws-cdk": "2.146.0",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "~5.4.5"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.602.0",
    "@aws-sdk/lib-dynamodb": "^3.602.0",
    "@aws-sdk/util-dynamodb": "^3.602.0",
    "aws-cdk-lib": "2.146.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
